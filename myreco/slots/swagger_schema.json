{
    "/slots/": {
        "post": {
            "operationId": "swagger_insert",
            "parameters": [{
                "name": "Authorization",
                "in": "header",
                "required": true,
                "type": "string"
            },{
                "name": "body",
                "in": "body",
                "required": true,
                "schema": {"$ref": "#/definitions/schema_array"}
            }],
            "responses": {"201": {"description": "Created"}}
        },
        "get": {
            "parameters": [{
                "name": "Authorization",
                "in": "header",
                "required": true,
                "type": "string"
            },{
                "name": "store_id",
                "in": "query",
                "required": true,
                "type": "integer"
            }],
            "operationId": "swagger_get_all",
            "responses": {"200": {"description": "Got"}}
        }
    },
    "/slots/{id}/": {
        "parameters": [{
            "name": "id",
            "in": "path",
            "type": "integer",
            "required": true
        }],
        "patch": {
            "operationId": "swagger_update",
            "parameters": [{
                "name": "Authorization",
                "in": "header",
                "required": true,
                "type": "string"
            },{
                "name": "body",
                "in": "body",
                "required": true,
                "schema": {"$ref": "#/definitions/schema_without_required"}
            }],
            "responses": {"200": {"description": "Got"}}
        },
        "delete": {
            "parameters": [{
                "name": "Authorization",
                "in": "header",
                "required": true,
                "type": "string"
            }],
            "operationId": "swagger_delete",
            "responses": {"204": {"description": "Deleted"}}
        },
        "get": {
            "parameters": [{
                "name": "Authorization",
                "in": "header",
                "required": true,
                "type": "string"
            }],
            "operationId": "swagger_get",
            "responses": {"200": {"description": "Got"}}
        }
    },
    "definitions": {
        "schema_without_required": {
            "type": "object",
            "minProperties": 1,
            "additionalProperties": false,
            "properties": {
                "max_recos": {"type": "integer"},
                "name": {"type": "string"},
                "store_id": {"type": "integer"},
                "engine_id": {"type": "integer"},
                "fallbacks": {"$ref": "#/definitions/fallbacks"},
                "slot_variables": {"$ref": "#/definitions/slot_variables"}
            }
        },
        "schema_array": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": ["engine_id", "store_id", "slot_variables", "max_recos", "name"],
                "properties": {
                    "max_recos": {"type": "integer"},
                    "name": {"type": "string"},
                    "store_id": {"type": "integer"},
                    "engine_id": {"type": "integer"},
                    "fallbacks": {"$ref": "#/definitions/fallbacks"},
                    "slot_variables": {"$ref": "#/definitions/slot_variables"}
                }
            }
        },
        "fallbacks": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": ["id"],
                "properties": {
                    "id": {"type": "integer"}
                }
            }
        },
        "slot_variables": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "minProperties": 2,
                "oneOf": [
                    {"required": ["id", "_operation"]},
                    {"required": ["variable_name", "variable_store_id", "inside_engine_name"]},
                    {"required": ["variable", "inside_engine_name"]}
                ],
                "properties": {
                    "id": {"type": "integer"},
                    "slot_id": {"type": "integer"},
                    "variable_name": {"type": "string"},
                    "variable_store_id": {"type": "integer"},
                    "_operation": {"enum": ["delete", "update", "insert"]},
                    "inside_engine_name": {"type": "string"},
                    "is_filter": {"type": "boolean"},
                    "filter_type": {"enum": ["By Property", "By Property Of"]},
                    "is_inclusive_filter": {"type": "boolean"},
                    "override": {"type": "boolean"},
                    "override_value": {"type": "string"},
                    "skip_values": {
                        "oneOf": [
                            {"type": "array"},
                            {"type": "null"}
                        ]
                    },
                    "variable": {
                        "type": "object",
                        "additionalProperties": false,
                        "oneOf": [
                            {"required": ["id"]},
                            {"required": ["name", "engine_id", "_operation"]}
                        ],
                        "properties": {
                            "id": {"type": "integer"},
                            "engine_id": {"type": "integer"},
                            "name": {"type": "string"},
                            "_operation": {"enum": ["remove", "insert", "get"]}
                        }
                    }
                }
            }
        }
    }
}